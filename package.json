{
  "name": "kustosz-web-ui",
  "version": "22.08.0",
  "engines": {
      "node": ">=18.0"
  },
  "description": "Focus on the worthwhile content with Kustosz, open source self-hosted web application. This package contains frontend code.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KustoszApp/web-ui.git"
  },
  "author": "Mirek DÅ‚ugosz <mirek@mirekdlugosz.com> (https://mirekdlugosz.com)",
  "license": "EUPL-1.2",
  "bugs": {
    "url": "https://github.com/KustoszApp/web-ui/issues"
  },
  "homepage": "https://www.kustosz.org/",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "cp ./vue-modal-postcss.config.js ./node_modules/@kouts/vue-modal/postcss.config.js && vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@kouts/vue-modal": "^3.0.8",
    "@vueform/multiselect": "^2.6.0",
    "axios": "^1.3.0",
    "bootstrap-icons-vue": "^1.10.0",
    "register-service-worker": "^1.7.0",
    "sanitize.css": "^13.0.0",
    "vue": "^3.2.0",
    "vue-router": "^4.1.0",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-pwa": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "prettier": "^2.0.0",
    "sass": "^1.60.0",
    "sass-loader": "^13.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {
      "vue/multi-word-component-names": [
        "error",
        {
          "ignores": [
            "Collapse",
            "Entries",
            "Entry",
            "Filters",
            "Login",
            "Maintenance",
            "Settings",
            "Sidebar",
            "Spinner"
          ]
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
